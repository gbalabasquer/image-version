
{
    "name": "imageversion/image-version",
    "description": "CakePHP 3 ImageVersion Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "cakephp3", "imageVersion"],
    "license": "MIT",
    "authors": [
	{
	    "name": "Tom Maiaroto (Adapted by: Gonzalo Balabasquer)",
	    "email": "gbalabasquer@gmail.com"
	}
    ],
    "require": {
	"php": ">=5.4",
	"facebook/php-sdk-v4": "~5.0"
    },
    "require-dev": {
	"phpunit/phpunit": "*"
    },
    "autoload": {
	"psr-4": {
	    "ImageVersion\\": "src"
	}
    },
    "autoload-dev": {
	"psr-4": {
            "ImageVersion\\Test\\": "tests",
	    "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
	}
    },
    "extra": {
	"installer-name": "ImageVersion"
    }
}